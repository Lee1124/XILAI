/* 图片样式 */
@import "/pages/template/template.wxss";
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.nav{
    width: 100%;
    height: 86rpx;
    background: #F4F4F4;
    display: flex;
    justify-content: flex-start;
    z-index:999;
position:fixed;
}
.nav-old{
    font-size: 28rpx;
    height: 87rpx;
    line-height: 87rpx;
    margin-left: 208rpx;
}
.card-image-point{
    width: 36rpx;
    /*height: 8rpx;*/
    float: right;
}

.active{
    color:#EB8778;
    box-sizing: border-box;
    border-bottom: 1px solid #EB8778;
}
.nav-new{
    font-size: 28rpx;
    height: 87rpx;
    line-height: 87rpx;
    margin-left: 150rpx;
}


.Cases{

}
image{
  width:100%;
}
@keyframes fadeIn {
    0% { opacity: 0; }
    100% { opacity: 1; }
}
.fade_in {
    animation: fadeIn 1s both;
}
.case_content{
  padding:30rpx 0 25rpx 35rpx;
}
.case_bt text{
   color:#222222;
   font-size: 32rpx;
   display: block;
   margin-bottom: 1%;
}
.case_bt{
  margin-top:-2%;
}
.case_ct text{
  color:#999999;
  font-size: 24rpx;
  padding-right:6rpx;
}
.content{
  padding-bottom: 120rpx;
  padding-top: 64px;
  overflow: hidden;
}


/*ys-最新案例*/
.content-new{
  width: 100%;
  padding-bottom: 180rpx;
  padding-top:100px;

}
.border5{
    height: 5px;
}
.case-list{
  width:100%;
padding:10rpx 20rpx;
box-sizing:border-box;
background:#fff;
border-bottom:10px solid #f2f2f2;
}
.case-title{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 20rpx;
}
.case-edit-image{
    width: 40rpx;
    height: 60rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.case-text{
    font-size: 32rpx;
    color:#222;
    margin-bottom: 10rpx;
}
.case-edit-image view{
    width: 8rpx;
    height: 8rpx;
    border-radius: 50%;
    background: #bbb;
}
.case-content{
    font-size: 28rpx;
    color:#666;
}
.image-list{
    display: flex;    
    width: 100%;
    margin-top: 20rpx;   
    flex-wrap: wrap;
}
.image-list image{
  width:32.6%;
  height:232rpx;
  margin-bottom:9rpx;
  margin-right:1%;
}
.image-list image:nth-child(3n){
  margin-right: 0;
}
.video, .showVideoImg{
  position:absolute;
  width:100%;
  height:0;
  padding-bottom: 56.25%;
  z-index: 1 !important;
}
.video video,.showVideoImg .model-img{
    /* width: 100%; */
    /* margin-top: 20rpx; */
    /* height: 300rpx; */
    /* z-index:0 !important; */

    position:absolute;
    left:0;
    top:0;
    width:100%;
    height:100%;
}
.case-footer{
    height: 60rpx;
    line-height: 60rpx;
    width: 100%;
    margin-top: 10rpx;

}
.case-time{
    display: inline-block;
    font-size: 22rpx;
    color:#999;
}
.case-share{
    float: right;
    font-size: 24rpx;
    display: inline-block;
    color:#999999;
    margin-right: 60rpx;
}
.talk{
    float: right;
    color:#999999;
    display: inline-block;
    font-size: 24rpx;
}
.icon{
    width: 30rpx;
    height: 30rpx;
    margin-right: 18rpx;
}
.add-case-btn{
    position: fixed;
    bottom: 98rpx;
    left: 0;
    width: 100%;
    height: 80rpx;
    background:#EB8778;
    line-height: 80rpx;
    text-align: center;
    color:#fff;
    font-size: 28rpx;
    z-index:99999999;
    border-radius: 0px;
}
.video{
    margin-top:20rpx;
    position: relative;
}
.model-img{
    width: 100%;
    /* height: 300rpx; */
    /* height: 56.25%; */
}
.model-btn{
    position:absolute;
    left:0;
    top:0;
    bottom:0;
    right:0;
    margin:auto;
    width:80rpx;
    height:80rpx;
    border-radius:50%;
    background-color: rgba(0,0,0,.3);
}
.play-icon{
  margin:27rpx 35rpx;
  border-top:15rpx solid transparent;
  border-left:20rpx solid #fff;
  border-bottom:15rpx solid transparent;

}
/*头部搜索*/
.search-div{
  padding:5rpx 20rpx;
  position:absolute;
  z-index:999;
  border:1px solid #efefef;
  top: 60px;
  left: 20rpx;
  right: 20rpx;
  box-sizing: border-box;
  height:40px;
  line-height:40px;
}
.search-div image{
  width:38rpx;
  height:38rpx;
  position:absolute;
  top:20rpx;
}
.search-div input{
  font-size:14px;
  height:80%;
  width:90%;
  display:inline-block;
  margin-left:27px;
  margin-top:3px;
  position:absolute;
}
/*回到顶部*/
.go_top{
  position:fixed;
  right:12px;
  bottom:90px;
  font-size:14px;
  background:rgba(255,255,255,0.9);
  border-radius:100%;
  padding:5px 5px;
  z-index:111111;
  text-align:center;
  height:48px;
  width:48px;
  color:#777;
  box-shadow:1px 0px 10px rgba(0,0,0,0.4);
}
.go_top image{
  width:25px;
  height:25px;
}
.noContent{
  width:100%;
  text-align:center;
  padding-top:70%;
  color:#bbb;
  font-size:30rpx;
  letter-spacing:1rpx;

}

