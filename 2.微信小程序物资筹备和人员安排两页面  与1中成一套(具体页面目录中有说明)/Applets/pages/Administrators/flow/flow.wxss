/* pages/Administrators/flow/flow.wxss */

.li{
    height:86rpx;
    border-bottom: 1px solid rgba(221,221,221,1);
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0px 20rpx;
}
.li>view:nth-child(1){
    font-size: 28rpx;
    color: #222222;
}
.li>view:nth-child(2){
    font-size: 24rpx;
    color: #222222;
}

.li .left_1{
    width:17rpx;
    height:30rpx;
}
.title_1{
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0px 23rpx 0px 20rpx;
    border-bottom: 1px solid rgba(221,221,221,1);
}
.title_1>view:nth-child(1)>text:nth-child(1){
    color: #000000;
    font-size: 28rpx;
}
.title_1>view:nth-child(1)>text:nth-child(2){
    color: #FF0000;
    font-size: 24rpx;
}
.title_1>view:nth-child(2){
    color: #999999;
    font-size: 24rpx
}
.border_bottom{
     border-bottom: 1px solid rgba(221,221,221,1);
}
.list_1{
    /* border-bottom: 1px solid rgba(221,221,221,1); */
}
.list_1 .list_2{ 
    /* max-height: 0px; */
}
.list_1.active .list_2{ 
    /* max-height: 100px; */
}
.list_2{
    padding:  0px 23rpx 0px 80rpx;
    overflow: hidden;
    transition: all 0.3s;
}
.list_2>view:last-child{
    border-bottom: none;
}
.list_2>view{
    border-bottom: 1px dashed rgba(221,221,221,1);
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #656565;
    font-size: 28rpx;
}
.list_2>view image{
    width:17rpx;
    height:30rpx;
}


