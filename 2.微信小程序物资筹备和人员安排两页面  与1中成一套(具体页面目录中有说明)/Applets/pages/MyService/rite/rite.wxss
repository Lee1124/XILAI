/*当天流程*/
.team-select{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color:#BB9860;
    font-size: 24rpx;
    background: #ddd;
}

/*一张卡片*/
.flow-card{
    width: 690rpx;
    margin: 0 auto;
}
/*卡片头部*/
.card-header{
    width: 100%;
    height: 82rpx;
    line-height: 82rpx;
    border: 1px solid #ddd;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    background: #F6F6F6;
    margin-top: 10rpx;
}
.card-icon{
    width: 40rpx;
    height: 40rpx;
    line-height: 38rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.card-image-icon{
    width: 38rpx;
    height: 38rpx;
}
.card-time,.card-flowname{
    font-size: 26rpx;
    color:#000;
}
.card-flowname{
    margin-left: 30rpx;
}
.card-point{
    flex: 1;
    height: 38rpx;
    line-height: 38rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}
.card-image-point{
    width: 36rpx;
    float: right;
    margin-right: 30rpx;
}


.arrow-down{
    width: 16rpx;
    height: 10rpx;
    margin-left: 10rpx;
}
.card-main{
    padding:0 30rpx 30rpx;
    border: 1px solid #DDDDDD;
    border-top: none;
}
.card-li{
    margin-top: 15rpx;
    margin-bottom: 15rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    font-size: 24rpx;
    color:#666666;
}
.card-li:first-child{
    margin-top: 0;
}
.card-li:last-child{
    margin-bottom: 0;
}
.card-up-left,.card-up-right{
    display: flex;
    align-items: center;
    width: 313rpx;
    color:#666666;
    line-height: 62rpx;
}
.card-down{
    width: 630rpx;
    line-height: 62rpx;
    display: flex;
    align-items: center;
    color:#666;
    margin-top: 10rpx;
}
.sinput{
    border-bottom: 1px solid #ddd;
    font-size: 24rpx;
    color:#666;
    height: 50rpx;
    width: 70%;
    display: inline-block;
    padding-left: 10rpx;
}
