}
.animate1{
    /* height: 208rpx; */
    /* height: 420rpx; */
    transition:height 0.4s;
    overflow: hidden;
    transition-timing-function: linear;
}
.animate2{
    height: 0;
    padding-bottom: 0;
    overflow: hidden;
    transition:height 0.4s;
    transition-timing-function: linear;
}
.mt{
    margin-top: 30rpx;
}
