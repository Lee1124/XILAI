/*引用输入框样式*/
.shadw_box {
    background: rgba(0, 0, 0, 0.80);
    position: fixed;
    width: 100%;
    top:0;
    left: 0;
    height: 100%;
    z-index: 1000;
}

.shadw_box>view {
    background: white;
    height: 562rpx;
    width: 390rpx;
    position: fixed;
    margin-left: auto;
    margin-right: auto;
    padding: 70rpx 55rpx 0 55rpx;
    left: 0;
    right: 0;
    bottom: 400rpx;
    border-radius: 5px;
}

.shadw_box>image {
    width: 70rpx;
    height: 70rpx;
    position: fixed;
    bottom: 295rpx;
    margin-left: auto;
    margin-right: auto;
    left: 0;
    right: 0;
}

.shadw2 {
    height: 662rpx !important;
    bottom: 300rpx !important;
    padding-top: 50rpx !important;
    overflow: visible;
}

.shadw2_image {
    bottom: 195rpx !important;
}

.shadw2 label {
    font-size: 30rpx;
    padding-bottom: 20rpx;
    padding-left: 4%;
    border-bottom: 1px dashed #ddd;
    width: 96%;
    display: block;
    margin-bottom: 20rpx;
}

.shadw2 label radio {
    width: 33px;
}

/* .shadw2>label:last-child {
  border: none;
} */

.shadw_box>view>text {
    font-size: 30rpx;
    text-align: center;
    color:#bb9860;
    padding-bottom: 20rpx;
    padding-left: 4%;
    border-bottom: 1px solid #ddd;
    width: 96%;
    display: block;
    margin-bottom: 20rpx;
}
button:after{
    border:none
}
.shadw_box>view>text:last-child {
    border: none;
}

.change_click {
    color: #ce1131;
}

