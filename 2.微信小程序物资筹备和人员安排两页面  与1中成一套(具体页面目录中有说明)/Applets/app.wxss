/**app.wxss**/
@import "/utils/zan.wxss";
@font-face {
    font-family: 'PingFang SC';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('PingFangSCRegular.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
page{
  width: 100%;
  height:100%;
  font-family:"PingFang SC";
}

.img-fix-width{
    width: 100%;
    /*display: block;*/
}

/*红色按钮*/
.redBtn{
    width: 100%;
    height: 88rpx;
    text-align: center;
    line-height: 88rpx;
    color:#fff;
    background: #CD1130;
    font-size: 30rpx;
    position: fixed;
    bottom: 0;
    border-radius: 0;
}
.redBtn:after{
    border: none;
}
image{
  vertical-align:middle;
}